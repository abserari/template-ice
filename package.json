{
  "name": "ncepu",
  "version": "0.1.0",
  "description": "轻量级模板，使用 JavaScript，仅包含基础的 Layout。",
  "dependencies": {
    "@alifd/next": "^1.19.4",
    "@alifd/theme-design-pro": "^0.x",
    "axios": "^0.21.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "build-plugin-fusion": "^0.1.0",
    "build-plugin-moment-locales": "^0.1.0",
    "eslint": "^6.8.0",
    "gh-pages": "^3.1.0",
    "ice.js": "^1.0.0",
    "stylelint": "^13.2.0"
  },
  "scripts": {
    "start": "icejs start",
    "build": "icejs build",
    "deploy": "cnpm run build&&gh-pages -d build",
